    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.9.5",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.4",
    "@fortawesome/fontawesome-free-brands": "^5.0.8",
    "@fortawesome/fontawesome-free-regular": "^5.0.8",
    "@fortawesome/fontawesome-free-solid": "^5.0.8",
    "@fortawesome/react-fontawesome": "0.0.17",
    "bootstrap": "^4.0.0-beta.2",
    "d3": "^4.13.0",
    "es6-promise": "^4.1.1",
    "exports-loader": "^0.6.4",
    "history": "^4.7.2",
    "imports-loader": "^0.7.1",
    "promise-polyfill": "6.0.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "4.2.2",
    "react-transition-group": "^1.2.0",
    "reactstrap": "^5.0.0-beta.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^4.4.1",
    "redux-thunk": "^2.2.0",
    "sass-resources-loader": "^1.3.3",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "start": "webpack-dev-server --progress --watch --colors --inline --history-api-fallback --config webpack.dev.js",
    "dev": "webpack -d --progress --watch --profile --json > compilation-stats.json --env.dev --config webpack.dev.js",
    "build": "webpack -p --progress --env.prod --config webpack.prod.js",
    "test": "jest",
    "test:coverage": "npm test -- --coverage",
    "test-coveralls:coverage": "npm test --  --coverage && cat ./coverage/lcov.info | coveralls",
    "test:watch": "npm test -- --watch",
    "clean": "rimraf ./build"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 5.0.0"
  },
  "jest": {
    "setupFiles": [
      "jest-localstorage-mock",
      "./scripts/jest/setupJest.js/",
      "./scripts/jest/shim.js",
      "./scripts/jest/enzymeSetup.js"
    ],
    "moduleNameMapper": {
      "@(.*)$": "<rootDir>/src/$1",
      "^Utils$": "<rootDir>/src/utils/utils",
      "Public(.*)$": "<rootDir>/$1",
      "Components(.*)$": "<rootDir>/src/components/$1",
      "^Actions(.*)$": "<rootDir>/src/actions/$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/src/__mocks__/styleMock.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/src/config/test.js",
      "<rootDir>/node_modules/"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  }
}
