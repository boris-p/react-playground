    "jest-fetch-mock": "^1.4.2",
    "jest-localstorage-mock": "^2.2.0",
    "node-sass": "^4.5.3",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.0.0",
    "redux-mock-store": "^1.3.0",
    "sass-loader": "^6.0.6",
    "source-list-map": "^2.0.0",
    "style-loader": "^0.18.2",
    "uglify-js": "^2.8.29",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "url-loader": "^0.5.9",
